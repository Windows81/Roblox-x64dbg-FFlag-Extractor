{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "PowerShell Launch Test Script",
			"type": "PowerShell",
			"request": "launch",
			"script": "${workspaceFolder}/test/test.ps1",
		},
		{
			"name": "Python Debugger: With Arguments",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}/main.py",
			"console": "integratedTerminal",
			"args": [
				"${command:pickArgs}",
				"-df",
				"-ds",
			],
		}
	]
}